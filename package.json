{
  "name": "@ckpack/mock-axios",
  "version": "0.0.4",
  "description": "Mock axios requests for testing and development",
  "type": "module",
  "main": "dist/mock-axios.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mock-axios.es.js",
      "require": "./dist/mock-axios.cjs.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "lint": "eslint . --ext .js,.ts --fix",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "pretest": "vite build",
    "release": "standard-version"
  },
  "devDependencies": {
    "vite": "^2.8.6",
    "typescript": "^4.6.2",
    "eslint": "^8.11.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-config-airbnb-typescript": "^16.1.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "vite-plugin-dts": "^0.9.10",
    "jest": "^27.5.1",
    "@types/jest": "^27.4.1",
    "ts-jest": "^27.1.3",
    "husky": "^7.0.4",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "lint-staged": "^12.3.6",
    "standard-version": "^9.3.2",
    "@ckpack/fetch-helper": "file:./",
    "axios": "^0.26.1"
  },
  "author": "陈锴",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/ckpack/mock-axios",
  "homepage": "https://github.com/ckpack/mock-axios",
  "keywords": [
    "mock",
    "axios",
    "mock-axios"
  ],
  "files": [
    "src/*",
    "dist/*"
  ],
  "dependencies": {},
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,tsx,jsx,vue}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  }
}
